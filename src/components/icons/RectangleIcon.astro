---
interface Props {
  class?: string;
  width: number;
  height: number;
}

const { class: className, width, height } = Astro.props;
---

<i class:list={["icon", className]}>
  <svg
    width={width}
    height={height}
    aria-label="A rectangle icon"
    viewBox=`0 0 ${width} ${height}`
    fill="none"
    xmlns="http://www.w3.org/2000/svg"
  >
    <rect width={width} height={height} fill="currentColor"></rect>
  </svg>
</i>
